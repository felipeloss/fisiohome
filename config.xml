<?xml version='1.0' encoding='utf-8'?>
<widget id="me.agenciaprime.fisio" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">

    <name>FisioHome</name>
    <description>
      Fisitorapia em casa.
    </description>
    <author email="felipe@agenciapri.me" href="http://agenciapri.me">
        Felipe loss
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
